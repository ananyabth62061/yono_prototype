    font-size:.35rem;
    text-align:center;
    margin: .15rem .15rem 0 .15rem;
    padding: .06rem 0;
    border-radius:.1rem;
    border:2px solid #000;
    font-weight:600;
    color:#fff;
}



.AppAllRummy_Notice_Slider {
